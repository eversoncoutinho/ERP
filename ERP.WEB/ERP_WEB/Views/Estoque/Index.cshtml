@model IEnumerable<EstoqueNomeQuantVM>

@{
    ViewData["Title"] = "Estoque";
}

<h1>Estoque</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(m=>m.Nome)
            </th>
            <th>
                @Html.DisplayNameFor(m=>m.Quantidade)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Quantidade)
            </td>
        </tr>
}
    </tbody>
</table>
